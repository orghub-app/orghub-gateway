{
  "name": "orghub-gateway",
  "version": "1.0.0",
  "description": "orghub-gateway",
  "main": "dist",
  "scripts": {
    "dev": "nodemon -w src --exec \"babel-node ./src\"",
    "build": "babel src -s -D -d dist",
    "start": "NODE_ENV=production node dist",
    "prestart": "npm run -s build",
    "test": "NODE_ENV=test mocha --timeout 10000 --require babel-core/register",
    "coverage": "nyc --temp-directory ./test/coverage/tmp npm test",
    "coveralls": "nyc report --temp-directory ./test/coverage/tmp npm test --reporter=text-lcov | coveralls",
    "lint": "eslint src test --ignore-pattern test/coverage*"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ollelauribostrom/orghub-gateway.git"
  },
  "keywords": [],
  "author": "Olle Lauri BostrÃ¶m (https://github.com/ollelauribostrom)",
  "license": "MIT",
  "dependencies": {
    "cors": "^2.8.4",
    "dotenv": "^5.0.1",
    "express": "^4.16.2",
    "express-rate-limit": "^2.11.0",
    "helmet": "^3.9.0",
    "http-proxy-middleware": "^0.17.4",
    "morgan": "^1.9.0",
    "rotating-file-stream": "^1.3.4"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.0.3",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "chai": "^4.1.2",
    "chai-http": "^3.0.0",
    "coveralls": "^3.0.0",
    "eslint": "^4.13.0",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-jsx-a11y": "^6.0.2",
    "eslint-plugin-react": "^7.5.1",
    "mocha": "^3.2.0",
    "nodemon": "^1.12.5",
    "nyc": "^11.3.0",
    "proxyquire": "^1.8.0",
    "sinon": "^4.1.3"
  },
  "engines": {
    "node": ">=8.9.1"
  },
  "nyc": {
    "report-dir": "test/coverage",
    "cache": true,
    "reporter": [
      "text",
      "html"
    ]
  },
  "bugs": {
    "url": "https://github.com/ollelauribostrom/orghub-gateway/issues"
  },
  "homepage": "https://github.com/ollelauribostrom/orghub-gateway#readme"
}
